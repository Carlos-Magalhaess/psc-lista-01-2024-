programa {
  funcao inicio() {
    cadeia nome
    inteiro idade
    real altura

    escreva("Digite seu nome:")
    leia(nome)
    escreva("Digite sua idade:")
    leia(idade)
    escreva("Digite sua altura:")
    leia(altura)
    escreva("Nome:", nome)
    escreva("\nIdade:", idade)
    escreva("\nAltura:", altura)
  }
}
